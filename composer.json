{
    "name": "vkbansal/spectrum",
    "description": "syntax highlighter in php",
    "keywords": [
        "syntax",
        "highlighter",
        "syntax-highlighter"
    ],
    "homepage": "https://github.com/vkbansal/spectrum",
    "license": "MIT",
    "authors": [
        {
            "name": "Vivek Kumar Bansal",
            "email": "contact@vkbansal.me",
            "homepage": "http://vkbansal.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "symfony/dom-crawler": "2.6.*",
        "symfony/css-selector": "2.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "symfony/var-dumper": "2.6.*",
        "league/commonmark": "0.7.*"
    },
    "autoload": {
        "psr-4" : {
            "VKBansal\\Spectrum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VKBansal\\Test\\Spectrum\\": "test/"
        }
    },
    "suggest": {
        "league/commonmark": "To be used with markdown"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
