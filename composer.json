{
    "name": "vkbansal/prism",
    "description": "Implementation of prismjs syntax highlighter in php",
    "keywords": ["syntax", "highlighter", "syntax-highlighter"],
    "require": {
        "php" : ">=5.4.0",
        "symfony/dom-crawler": "~2.6.0",
        "symfony/css-selector": "~2.6.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vkbansal/prism.php"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "symfony/finder": "2.6.*",
        "twig/twig": "1.16.*",
        "erusev/parsedown-extra": "*",
        "vkbansal/frontmatter": "~1.3.0"
    },

    "autoload": {
        "psr-4" : {
            "VKBansal\\Prism\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vivek Kumar Bansal",
            "email": "vkb0310@gmail.com"
        }
    ]
}
